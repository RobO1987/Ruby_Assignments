<h1><%= @student.dojo.branch %> <%= @student.first_name%> <%= @student.last_name %></h1>
<p>Email: <%= @student.email %></p>
<br>
<br>
<h2>Cohort</h2>
<br>
<table>
  <thead>
    <tr>
      <th>First Name:</th>
      <th>Last Name:</th>
      <th>Email:</th>
    </tr>
  </thead>

  <tbody>
    <% @cohorts.each do |cohort| %>
    <tr>
      <td><%= cohort.first_name %></td>
      <td><%= cohort.last_name %> </td>
      <td><%= cohort.email %></td>
    </tr>
    <% end %>
  </tbody>
</table>
<br>
<br>
<a href="/student/<%= @student.id %>/edit">Edit</a>
