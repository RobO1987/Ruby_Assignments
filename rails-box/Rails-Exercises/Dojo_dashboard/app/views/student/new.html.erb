<style media="screen">
.form input {
  display: list-item;
}
</style>
<h1>New Student</h1>

<form class="form" action="/create" method="post">

<input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">

First Name: <input type="text" name='student[first_name]' value="">

Last Name: <input type="text" name="student[last_name]" value="">

Email Address: <input type="text" name="student[email]" value="">

<p>Location:</p>
<select name="student[dojo_id]">
  <% @AllDojo.each do |dojo| %>
  <option value="<%= dojo.id %>"><%= dojo.branch %></option>
  <% end %>
</select>


  <br>
  <br>




  <input type="submit" name="submit" value="Add Student">

</form>
